{
  "background": {
    "scripts": ["background.js"]
  },
  "description": "A web extension to find the visited youtube videos from the history.",
  "page_action": {
    "default_title": "My Youtube History",
    "default_popup": "history.html"
  },
   "icons": {
    "16": "16p.png",
    "48": "48p.png",
    "128": "128p.png"
  },
  "permissions": [
    "activeTab",
    "history",					
    "tabs"
  ],
  "manifest_version": 2,
  "name": "History List",
  "version": "0.1.1"
}
