{
  "name": "PV",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension for adding youtube URL to a database",
  "homepage_url": "https://parth-vader.github.io/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "history",
    "geolocation",
    "https://www.youtube.com/*",
    "tabs"
  ]
}